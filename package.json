{
  "name": "main-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "postinstall": "package-scripts/post-install.js",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "app:command": "ts-node src/cli.ts"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/terminus": "^7.2.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "connect-flash": "^0.1.1",
    "express-session": "^1.17.2",
    "handlebars-layout": "^0.1.4",
    "hbs": "^4.1.2",
    "helmet": "^4.6.0",
    "moment": "^2.29.1",
    "mysql2": "^2.3.0",
    "nestjs-command": "^2.0.1",
    "nestjs-sequelize-paginate": "^1.0.0",
    "nestjs-url-generator": "^0.0.9",
    "nodemailer": "^6.6.3",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^2.1.0",
    "session-file-store": "^1.5.0",
    "swagger-ui-express": "^4.1.6",
    "ts-enum-util": "^4.0.2",
    "uuid": "^8.3.2",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/connect-flash": "^0.0.37",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/hbs": "^4.0.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/sequelize": "^4.28.10",
    "@types/session-file-store": "^1.2.2",
    "@types/supertest": "^2.0.11",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "27.0.6",
    "mockdate": "^3.0.5",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.module.(t|j)s",
      "!databases/migrations/*.(t|j)s",
      "!databases/models/**/*.(t|j)s",
      "!databases/config.(t|j)s",
      "!documentation-helper/**/*.(t|j)s",
      "!environment/configs/*.(t|j)s",
      "!main.(t|j)s",
      "!cli.(t|j)s",
      "!migration.(t|j)s",
      "!cli-commands/**/*.(t|j)s",
      "!cluster/**/*.(t|j)s",
      "!helpers/filters/error-validation-format/**/*.(t|j)s",
      "!helpers/utils/check-env-file.(t|j)s",
      "!helpers/interceptors/context/**/*.(t|j)s",
      "!health/controllers/health/**/*.(t|j)s",
      "!common/services/transaction-provider/**/*.(t|j)s",
      "!services/logging/**/*.(t|j)s",
      "!services/url-config-service/**/*.(t|j)s",
      "!mail/services/mail-config/**/*.(t|j)s",
      "!mail/services/mail-config/**/*.(t|j)s",
      "!transaction-manager/services/transaction-provider/**/*.(t|j)s",
      "!view-engine/services/view-engine-config/**/*.(t|j)s",
      "!session-manager/services/session-config/**/*.(t|j)s",
      "!**/decorators/**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
