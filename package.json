    "@types/jest": "^29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.17",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/sequelize": "^4.28.10",
